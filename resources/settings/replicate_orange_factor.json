{
	"primary_field": "timestamp",
	"last_replication_time": "./results/data/orange_factor_last_replication_time.txt",
	"batch_size": 100,
	"fix":{
		"bug":"int(bug)",
		"starttime":"float(starttime)",
		"timestamp":"float(timestamp)",
		"date":"Date(date).unix",
		"push_date":"hg.get_revision(wrap({\"changeset\":{\"id\":rev}, \"branch\":{\"name\": tree.split('/')[-1]}})).push.date"
	},
	"hg":{
		"$ref":"//../../mohg/resources/settings.json"
	},
	"source": {
		"host": "http://of-elasticsearch-zlb.webapp.scl3.mozilla.com",
		"port": 9200,
		"index": "bugs",
		"type": "bug_info",
		"debug": false
	},
	"destination": {
//		"host": "http://activedata.allizom.org",
		"host": "http://localhost",
		"port": 9200,
		"index": "orange_factor",
		"type": "bug_info",
		"timeout": 300,
		"schema": {
			"$ref": "//../schema/orange_factor.json"
		},
		"debug": false,
		"limit_replicas": false,
		"zip": false
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"From": "https://wiki.mozilla.org/Auto-tools/Projects/OrangeFactor#ActiveData"
		},
		"pyLibrary.env.http.USE_COMPRESSION": false
	},
	"debug": {
		"log": [
			{
				"log_type": "stream",
				"stream": "sys.stdout"
			},
			{
				"class" : "logging.handlers.RotatingFileHandler",
				"filename" : "./results/logs/replicate_orange_factor.log",
				"maxBytes" : 10000000,
				"backupCount" : 10,
				"encoding" : "utf8"
			},
			{
				"log_type": "email",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][Toronto] Problem in replicate_orange_factor",
				"$ref": "file://~/private.json#email"

			}
		]
	}
}
