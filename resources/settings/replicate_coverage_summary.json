{
	"primary_field": "etl.timestamp",
	"primary_interval": 500000,
	"last_replication_time": "./results/data/coverage_last_replication_time.txt",
	"source": {
		"host": "http://activedata.allizom.org",
		"port": 9200,
		"index": "coverage-summary",
		"type": "code_coverage",
		"debug": false
	},
	"destination": {
		"host": "http://localhost",
		"port": 9200,
		"index": "coverage-summary",
		"type": "code_coverage",
		"timeout": 300,
		"schema": {
			"$ref": "//~/code/Activedata-ETL/resources/schema/code_coverage.json"
		},
		"debug": false,
		"limit_replicas": false,
		"zip": false
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		}
	},
	"debug": {
		"log": [
			{
				"log_type": "stream",
				"stream": "sys.stdout"
			},
			{
				"class" : "logging.handlers.RotatingFileHandler",
				"filename" : "./results/logs/replicate_coverage.log",
				"maxBytes" : 10000000,
				"backupCount" : 10,
				"encoding" : "utf8"
			},
			{
				"log_type": "ses",
				"$ref": "file://~/private.json#aws_credentials",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][Toronto] Problem in replicate_coverage"
			}
		]
	}
}
