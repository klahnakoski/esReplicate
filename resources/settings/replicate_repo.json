{
	"primary_field": "changeset.date",
	"primary_interval": 500000,
	"last_replication_time": "./results/data/repo_last_replication_time.txt",
	"insert_size": 100,
	"scan_size": 10000,
	"fix":{
		"changeset\\.moves": "None",
		"changeset\\.diff": "None"
	},
	"source": {
		"host": "http://activedata.allizom.org",
//		"host": "http://54.244.44.73",  // OLD CLUSTER
		"port": 9200,
		"index": "repo",
		"type": "revision",
		"debug": false
	},
	"destination": {
		"host": "http://localhost",
		"port": 9200,
		"index": "repo",
		"type": "revision",
		"typed": false,
		"timeout": 300,
		"schema": {
			"$ref": "../../vendor/mo_hg/repos/revision_schema.json"
		},
		"debug": false,
		"limit_replicas": true,
		"zip": false
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		}
	},
	"debug": {
		"trace": true,
		"log": [
			{
				"log_type": "stream",
				"stream": "sys.stdout"
			},
			{
				"class" : "logging.handlers.RotatingFileHandler",
				"filename" : "./results/logs/replicate_repo.log",
				"maxBytes" : 10000000,
				"backupCount" : 10,
				"encoding" : "utf8"
			},
			{
				"log_type": "ses",
				"$ref": "file://~/private.json#aws_credentials",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][Toronto] Problem in replicate_repo"
			}
		]
	}
}
